{
    "name": "rsa-gui",
    "description": "The GUI for RSA CLI.",
    "version": "0.1.0-beta.1",
    "author": {
        "name": "MrWillCom",
        "email": "mr.will.com@outlook.com",
        "url": "https://github.com/MrWillCom"
    },
    "private": true,
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "babel src --out-dir lib --copy-files",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "dist-win-ia32": "electron-builder --win --ia32",
        "dist-win-x64": "electron-builder --win --x64",
        "dist-win-arm64": "electron-builder --win --arm64",
        "dist-mac-x64": "electron-builder --mac --x64",
        "dist-mac-universal": "electron-builder --mac --universal",
        "dist-mac-arm64": "electron-builder --mac --arm64",
        "dist-linux-x64": "electron-builder --linux --x64"
    },
    "dependencies": {
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/runtime": "^7.14.8",
        "@electron/remote": "^1.2.1",
        "@vscode/codicons": "^0.0.21",
        "bootstrap-icons": "^1.5.0",
        "electron-store": "^8.0.0",
        "normalize.css": "^8.0.1",
        "qrcode": "^1.4.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rsa-cli": "^1.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "electron": "^13.1.8",
        "electron-builder": "^22.11.7"
    }
}
